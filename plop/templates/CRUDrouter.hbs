import { Router, Request, Response } from "express";

export var router_{{name}}: Router = Router()

router_{{name}}.get('/', (req: Request, res:Response) => {
    return res.sendStatus(200);
})

router_{{name}}.get('/:id', (req: Request, res:Response) => {
    return res.sendStatus(200);
})

router_{{name}}.post('/', (req: Request, res:Response) => {
    return res.sendStatus(200);
})

router_{{name}}.put('/:id', (req: Request, res:Response) => {
    return res.sendStatus(200);
})

router_{{name}}.delete('/:id', (req: Request, res:Response) => {
    return res.sendStatus(200);
})